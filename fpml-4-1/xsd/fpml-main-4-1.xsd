                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">Specifies the frequency at which the
                                bond pays, e.g. 6M.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element form="qualified" minOccurs="0" name="dayCountFraction"
                        type="DayCountFraction">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">The day count basis for the
