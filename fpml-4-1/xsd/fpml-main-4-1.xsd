    <xsd:complexType name="Termination">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">An event type that defines the content of a Termination
                transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Event">
                <xsd:sequence>
                    <xsd:group ref="TradeOrTradeReference.model"/>
                    <xsd:group ref="TerminationDetails.model"/>
                    <xsd:element form="qualified" minOccurs="0" name="payment" type="Payment">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">A payment for the right to terminate
                                the trade.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="TerminationConfirmed">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A message generated when a Termination is determined to
