                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">One party element for each of the
                                principal parties and any other party that is
                                referenced.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RequestNovationConfirmation">
        <xsd:complexContent>
            <xsd:extension base="RequestMessage">
                <xsd:sequence>
