        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:normalizedString">
                <xsd:attribute form="unqualified" name="languageScheme" type="xsd:anyURI"/>
            </xsd:extension>
        </xsd:simpleContent>
